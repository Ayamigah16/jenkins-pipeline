[jenkins]
jenkins_node ansible_host="{{lookup('env','JENKINS_HOST_IP')}}" ansible_user="{{lookup('env','ANSIBLE_SSH_USER')}}" ansible_ssh_private_key_file="{{lookup('env','ANSIBLE_SSH_PRIVATE_KEY_FILE')}}" ansible_ssh_common_args='-o StrictHostKeyChecking=no'

[deploy]
deploy_node ansible_host="{{lookup('env','DEPLOY_HOST_IP')}}" ansible_user="{{lookup('env','ANSIBLE_SSH_USER')}}" ansible_ssh_private_key_file="{{lookup('env','ANSIBLE_SSH_PRIVATE_KEY_FILE')}}" ansible_ssh_common_args='-o StrictHostKeyChecking=no'
